---
blog: reading
---

<h1>Reading List</h1>

<% if page_articles %>
<section class="reading-list">
  <h1 class="visuallyhidden">Archive</h1>
  <ol>
    <% page_articles.each_with_index do |article, i| %>
      <li>
          <% link_to article do %>
              <img src="<%= article.data.cover %>" alt="<%= article.title %>">
          <% end %>
      </li>
    <% end %>
  </ol>

<% end %>
